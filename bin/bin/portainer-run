#!/bin/bash

PROJECT_ROOT=$(git rev-parse --show-toplevel)
DIST=$PROJECT_ROOT/dist
PORTAINER=$DIST/portainer
DATA="$HOME/src/portainer/drives/current"
ASSETS=$DIST

$PORTAINER --data="$DATA" --assets="$ASSETS" --license-expire-absolute
# $PORTAINER --data="$DATA" --assets="$ASSETS" --log-level=DEBUG
